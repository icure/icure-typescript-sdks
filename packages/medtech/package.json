{
    "name": "@icure/medical-device-sdk",
    "version": "3.0.3",
    "description": "iCure SDK for medical device applications",
    "author": "iCure",
    "keywords": [
        "fetch",
        "typescript",
        "openapi-client",
        "openapi-generator"
    ],
    "license": "Unlicense",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "scripts": {
        "marshall": "nx run hephaestus:start --tsconfig ../medtech/tsconfig.json --model-regex '^.*/medtech/src/models/.*.model.ts$' --mapper-location ../medtech/src/mappers --marshaller-generator --mapper-generator",
        "prepare": "rimraf dist && yarn run build && jq '{name:.name, version:.version, description:.description, main:\"index.js\", types:\"index.d.ts\", dependencies:.dependencies, author:.author, license:.license, bugs:.bugs, homepage:.homepage}' < package.json > dist/package.json",
        "publish": "yarn run prepare && cd dist && npm publish && cd ..",
        "test": "jest --runInBand --no-cache test",
        "build": "tsc"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "reporter": [
            "html",
            "lcov"
        ],
        "all": true,
        "author": "iCure SA",
        "license": "MIT",
        "bugs": {
            "url": "https://github.com/icure/icure-medical-device-js-sdk/issues"
        },
        "homepage": "https://github.com/icure/icure-medical-device-js-sdk"
    },
    "dependencies": {
        "@icure/api": "7.1.8",
        "@icure/typescript-common": "1.1.1",
        "browser-or-node": "^2.1.1",
        "date-fns": "^2.28.0",
        "es6-promise": "^4.2.4",
        "loglevel": "^1.8.1",
        "node-jq": "^2.3.3",
        "uuid": "^8.3.2",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.5",
        "@babel/plugin-proposal-decorators": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@icure/test-setup": "^0.0.52",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.1",
        "@types/isomorphic-fetch": "^0.0.36",
        "@types/jest": "^29.5.2",
        "@types/md5": "^2.3.2",
        "@types/mocha": "^9.1.0",
        "@types/node-localstorage": "^1.3.0",
        "@types/uuid": "^8.3.4",
        "@types/ws": "^8.5.4",
        "axios": "^0.26.1",
        "babel-jest": "^29.5.0",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.5.0",
        "md5": "^2.3.0",
        "mocha": "^9.2.2",
        "node-localstorage": "^2.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.21",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-md5": "^1.3.1",
        "ts-node": "^10.7.0",
        "typedoc": "^0.23.15",
        "typedoc-plugin-markdown": "^3.13.6",
        "typescript": "^4.5.5"
    }
}
